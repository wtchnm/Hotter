{
	"name": "hotter",
	"version": "1.0.4",
	"license": "MIT",
	"scripts": {
		"commit": "git cz",
		"start": "cross-env NODE_ENV=development node --no-deprecation node_modules/webpack-dev-server/bin/webpack-dev-server.js --config ./.config/webpack/webpack.dev.js",
		"build": "cross-env NODE_ENV=production node --no-deprecation node_modules/webpack/bin/webpack.js --config ./.config/webpack/webpack.prod.js",
		"analyze": "cross-env NODE_ENV=production node --no-deprecation node_modules/webpack/bin/webpack.js --env.analyze --config ./.config/webpack/webpack.prod.js",
		"profile": "cross-env NODE_ENV=production node --no-deprecation node_modules/webpack/bin/webpack.js --env.profile --config ./.config/webpack/webpack.prod.js --profile --json > stats.json",
		"serve": "serve -s dist"
	},
	"devDependencies": {
		"@babel/core": "7.11.1",
		"@babel/plugin-proposal-class-properties": "7.10.4",
		"@babel/plugin-transform-runtime": "7.11.0",
		"@babel/preset-env": "7.11.0",
		"@babel/preset-react": "7.10.4",
		"@babel/preset-typescript": "7.10.4",
		"@pmmmwh/react-refresh-webpack-plugin": "0.4.1",
		"@types/copy-webpack-plugin": "6.0.0",
		"@types/mini-css-extract-plugin": "0.9.1",
		"@types/react": "16.9.46",
		"@types/react-dom": "16.9.8",
		"@types/webpack-bundle-analyzer": "3.8.0",
		"@types/workbox-webpack-plugin": "5.1.4",
		"@typescript-eslint/eslint-plugin": "3.9.0",
		"@typescript-eslint/parser": "3.9.0",
		"babel-loader": "8.1.0",
		"clean-webpack-plugin": "3.0.0",
		"commitizen": "4.1.2",
		"copy-webpack-plugin": "6.0.3",
		"cross-env": "7.0.2",
		"css-loader": "4.2.1",
		"cssnano": "4.1.10",
		"cz-conventional-changelog": "3.2.0",
		"eslint": "7.6.0",
		"eslint-config-airbnb": "18.2.0",
		"eslint-config-prettier": "6.11.0",
		"eslint-plugin-babel": "5.3.1",
		"eslint-plugin-import": "2.22.0",
		"eslint-plugin-jsx-a11y": "6.3.1",
		"eslint-plugin-react": "7.20.5",
		"eslint-plugin-react-hooks": "4.0.8",
		"html-webpack-plugin": "4.3.0",
		"husky": "4.2.5",
		"lint-staged": "10.2.11",
		"mini-css-extract-plugin": "0.10.0",
		"postcss-loader": "3.0.0",
		"postcss-preset-env": "6.7.0",
		"prettier": "2.0.5",
		"react-refresh": "0.8.3",
		"serve": "11.3.2",
		"style-loader": "1.2.1",
		"stylelint": "13.6.1",
		"stylelint-config-prettier": "8.0.2",
		"stylelint-config-standard": "20.0.0",
		"stylelint-prettier": "1.1.2",
		"typescript": "3.9.7",
		"webpack": "5.0.0-beta.25",
		"webpack-bundle-analyzer": "3.8.0",
		"webpack-cli": "3.3.12",
		"webpack-dev-server": "3.11.0",
		"workbox-webpack-plugin": "5.1.3"
	},
	"dependencies": {
		"@babel/runtime-corejs3": "7.11.2",
		"core-js": "3.6.5",
		"react": "16.13.1",
		"react-dom": "16.13.1",
		"tailwindcss": "1.6.2"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged --config .config/.lintstagedrc.json"
		}
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"prettier": {
		"tabWidth": 4,
		"useTabs": true,
		"singleQuote": true
	},
	"browserslist": "Edge >= 15, Firefox >= 54, Chrome >= 51, Safari >= 10, Opera >= 38"
}
