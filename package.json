{
	"name": "hotter",
	"version": "1.0.4",
	"license": "MIT",
	"scripts": {
		"commit": "git cz",
		"start": "webpack-dev-server --config ./.config/webpack/webpack.dev.js",
		"build": "webpack --config ./.config/webpack/webpack.prod.js",
		"analyze": "webpack --env.analyze --config ./.config/webpack/webpack.prod.js",
		"profile": "webpack --env.profile --config ./.config/webpack/webpack.prod.js --profile --json > stats.json",
		"serve": "serve -s dist"
	},
	"devDependencies": {
		"@babel/core": "7.9.6",
		"@babel/plugin-transform-runtime": "7.9.6",
		"@babel/preset-env": "7.9.6",
		"@babel/preset-react": "7.9.4",
		"@babel/preset-typescript": "7.9.0",
		"@types/node": "13.13.5",
		"@types/react": "16.9.35",
		"@types/react-dom": "16.9.8",
		"@types/workbox-webpack-plugin": "5.1.0",
		"@typescript-eslint/eslint-plugin": "2.31.0",
		"@typescript-eslint/parser": "2.31.0",
		"@yarnpkg/pnpify": "2.0.0-rc.22",
		"babel-loader": "8.1.0",
		"clean-webpack-plugin": "3.0.0",
		"commitizen": "4.1.2",
		"css-loader": "3.5.3",
		"cssnano": "4.1.10",
		"cz-conventional-changelog": "3.2.0",
		"eslint": "7.0.0",
		"eslint-config-airbnb": "18.1.0",
		"eslint-config-prettier": "6.11.0",
		"eslint-import-resolver-webpack": "0.12.1",
		"eslint-plugin-import": "2.20.2",
		"eslint-plugin-jsx-a11y": "6.2.3",
		"eslint-plugin-prettier": "3.1.3",
		"eslint-plugin-react": "7.19.0",
		"eslint-plugin-react-hooks": "4.0.1",
		"html-webpack-plugin": "4.3.0",
		"husky": "4.2.5",
		"lint-staged": "10.2.2",
		"mini-css-extract-plugin": "0.9.0",
		"postcss-import": "12.0.1",
		"postcss-loader": "3.0.0",
		"postcss-preset-env": "6.7.0",
		"prettier": "2.0.5",
		"serve": "11.3.0",
		"style-loader": "1.2.1",
		"stylelint": "13.3.3",
		"stylelint-config-prettier": "8.0.1",
		"stylelint-config-standard": "20.0.0",
		"stylelint-prettier": "1.1.2",
		"typescript": "3.8.3",
		"webpack": "5.0.0-beta.16",
		"webpack-bundle-analyzer": "3.7.0",
		"webpack-cli": "3.3.11",
		"webpack-dev-server": "3.11.0",
		"webpackbar": "4.0.0",
		"workbox-webpack-plugin": "5.1.3"
	},
	"dependencies": {
		"@babel/runtime-corejs3": "7.9.6",
		"@hot-loader/react-dom": "16.13.0",
		"core-js": "3.6.5",
		"normalize.css": "8.0.1",
		"react": "16.13.1",
		"react-dom": "16.13.1",
		"react-hot-loader": "4.12.21"
	},
	"husky": {
		"hooks": {
			"pre-commit": "yarn lint-staged --config .config/.lintstagedrc.json"
		}
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"prettier": {
		"tabWidth": 4,
		"useTabs": true,
		"singleQuote": true
	},
	"stylelint": {
		"extends": [
			"stylelint-config-standard",
			"stylelint-prettier/recommended"
		]
	},
	"browserslist": "Edge >= 15, Firefox >= 54, Chrome >= 51, Safari >= 10, Opera >= 38"
}
