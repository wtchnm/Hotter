{
	"name": "hotter",
	"version": "1.0.4",
	"license": "MIT",
	"scripts": {
		"commit": "git cz",
		"start": "node --no-deprecation node_modules/webpack-dev-server/bin/webpack-dev-server.js --config ./.config/webpack/webpack.dev.js",
		"build": "node --no-deprecation node_modules/webpack/bin/webpack.js --config ./.config/webpack/webpack.prod.js",
		"analyze": "node --no-deprecation node_modules/webpack/bin/webpack.js --env.analyze --config ./.config/webpack/webpack.prod.js",
		"profile": "node --no-deprecation node_modules/webpack/bin/webpack.js --env.profile --config ./.config/webpack/webpack.prod.js --profile --json > stats.json",
		"serve": "superstatic dist"
	},
	"devDependencies": {
		"@babel/core": "7.9.0",
		"@babel/plugin-transform-runtime": "7.9.0",
		"@babel/preset-env": "7.9.0",
		"@babel/preset-react": "7.9.4",
		"@babel/preset-typescript": "7.9.0",
		"@types/react": "16.9.25",
		"@types/react-dom": "16.9.5",
		"@types/webpack": "4.41.8",
		"@typescript-eslint/eslint-plugin": "2.25.0",
		"@typescript-eslint/parser": "2.28.0",
		"babel-loader": "8.1.0",
		"clean-webpack-plugin": "3.0.0",
		"commitizen": "4.0.3",
		"css-loader": "3.4.2",
		"cssnano": "4.1.10",
		"cz-conventional-changelog": "3.1.0",
		"eslint": "6.8.0",
		"eslint-config-airbnb": "18.1.0",
		"eslint-config-prettier": "6.10.1",
		"eslint-plugin-import": "2.20.1",
		"eslint-plugin-jsx-a11y": "6.2.3",
		"eslint-plugin-prettier": "3.1.2",
		"eslint-plugin-react": "7.19.0",
		"eslint-plugin-react-hooks": "2.5.1",
		"html-webpack-plugin": "https://github.com/PutziSan/html-webpack-plugin.git",
		"husky": "4.2.3",
		"lint-staged": "10.0.9",
		"mini-css-extract-plugin": "0.9.0",
		"postcss-import": "12.0.1",
		"postcss-loader": "3.0.0",
		"postcss-preset-env": "6.7.0",
		"prettier": "2.0.2",
		"style-loader": "1.1.3",
		"stylelint": "13.2.1",
		"stylelint-config-prettier": "8.0.1",
		"stylelint-config-standard": "20.0.0",
		"stylelint-prettier": "1.1.2",
		"superstatic": "6.0.4",
		"terser-webpack-plugin": "2.3.5",
		"thread-loader": "2.1.3",
		"typescript": "3.8.3",
		"webpack": "5.0.0-beta.14",
		"webpack-bundle-analyzer": "3.6.1",
		"webpack-cli": "3.3.11",
		"webpack-dev-server": "3.10.3",
		"webpackbar": "4.0.0",
		"workbox-webpack-plugin": "5.1.2"
	},
	"dependencies": {
		"@babel/runtime": "7.9.2",
		"core-js": "3.6.4",
		"normalize.css": "8.0.1",
		"react": "16.13.1",
		"react-dom": "npm:@hot-loader/react-dom",
		"react-hot-loader": "4.12.20"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged --config .config/.lintstagedrc"
		}
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"prettier": {
		"tabWidth": 4,
		"useTabs": true,
		"singleQuote": true
	},
	"stylelint": {
		"extends": [
			"stylelint-config-standard",
			"stylelint-prettier/recommended"
		]
	}
}
